{
  "FunctionName": "grass-canvas-notifier",
  "Role": "{{ must_env `LAMBDA_ROLE_ARN` }}",
  "PackageType": "Image",
  "Code": {
    "ImageUri": "{{ must_env `IMAGE_URI` }}"
  },
  "Timeout": 30,
  "MemorySize": 512,
  "Environment": {
    "Variables": {
      "GITHUB_USERNAME": "{{ must_env `GITHUB_USERNAME` }}",
      "S3_BUCKET_NAME": "{{ must_env `S3_BUCKET_NAME` }}",
      "SSM_GITHUB_TOKEN_PATH": "{{ env `SSM_GITHUB_TOKEN_PATH` `/grass-canvas/github-token` }}",
      "SSM_LINE_CHANNEL_ACCESS_TOKEN_PATH": "{{ env `SSM_LINE_CHANNEL_ACCESS_TOKEN_PATH` `/grass-canvas/line-channel-access-token` }}",
      "SSM_LINE_USER_ID_PATH": "{{ env `SSM_LINE_USER_ID_PATH` `/grass-canvas/line-user-id` }}"
    }
  },
  "Tags": {
    "Project": "grass-canvas",
    "ManagedBy": "lambroll"
  }
}
